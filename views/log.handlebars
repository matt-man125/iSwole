<header id="header"></header>
<div class="pageGeneral" id="pageGeneral">
  <form action="/log" method="post">
    <h1>Info:</h1>
    <div class="containerLR">
      <h2 class="left">Body Weight (lbs):</h2>
      <div class="inputDiv right"><input type="number" id="bodyWeight" name="bodyWeight" placeholder="160 lbs" /></div>
    </div>
    <hr>
    <div class="containerLR">
      <h2 class="left">Start Time:</h2>
      <div class="inputDiv right"><input type="datetime-local" name="startTime"/></div>
    </div>
